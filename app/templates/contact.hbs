<ul class="list-unstyled">
  {{#each legislator in model}}
    <li>({{legislator.party}}) {{legislator.title }}.<span class="title"> {{legislator.first_name}} {{legislator.last_name}} </span>
      <br> Contact:
        <ul>
          <li>Phone: {{legislator.phone}}</li>
          <li>Office: {{legislator.office}}</li>
          <li><a href={{legislator.contact_form}}>{{legislator.contact_form}}</a></li>
        </ul>
      </li>
  {{/each}}
</ul>
